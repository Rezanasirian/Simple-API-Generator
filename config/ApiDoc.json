            "default_field": "cust_no",
            "default_direction": "ASC"
        },
        "LastUpdateTableName": "RPT_CUS_DepCustomer",
        "cache": {
            "enabled": false,
            "ttl_seconds": 3600
        },
        "Conditions": [
            {
                "BranchNo": {
                    "action": "update_condition",
                    "Parameter": "BranchNo",
                    "Name": "Branch Number",
                    "display_name": "Branch Number",
                    "Column": "branch_id",
                    "Operator": "in",
                    "IgnoreIf": "All",
                    "required": false,
                    "data_type": "string",
                    "category": "Filter",
                    "transformations": {
                        "cast": "varchar(20)"
                    }
                }
            },
            {
                "acct_no": {
                    "action": "update_condition",
                    "Parameter": "acct_no",
                    "Name": "Account Number",
                    "display_name": "Account Number",
                    "Column": "acct_no",
                    "Operator": "<",
                    "IgnoreIf": "-3",
                    "required": false,
                    "data_type": "string",
                    "category": "Identification",
                    "transformations": {
                        "substring": [
                            2,
                            11
                        ],
                        "replace": [
                            null,
                            null
                        ],
                        "trim": true,
                        "sqlCommand": "cast as int"
                    },
                    "validation": {
                        "min_length": 5,
                        "max_length": 20,
                        "pattern": "^[0-9]+"
                    }
                }
            }
        ],
        "response": {
            "fields": [
                "cust_no",
                "acct_no",
                "branch_id",
                "balance",
                "last_activity_date"
            ],
            "transformations": {}
        }
    },
